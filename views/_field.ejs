<div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusid"> ID</label>
    <div class="col-sm-7">
        <input autofocus class="form-control" name="cusid" id="cusid" value="<%= model.cusid %>" required />
    </div>
</div>

<div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusfname">First Name</label>
    <div class="col-sm-7">
        <input class="form-control" name="cusfname" value="<%= model.cusfname %>" required />
    </div>
</div>

<div class="form-group row">
    <label class="col-form-label col-sm-2" for="cuslname">Last Name</label>
    <div class="col-sm-7">
        <input class="form-control" name="cuslname" value="<%= model.cuslname %>" required />
    </div>
</div>

<div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusstate">State</label>
    <div class="col-sm-7">
        <input class="form-control" name="cusstate" maxlength="2" pattern="[a-zA-Z]{2}" value="<%= model.cusstate %>" />
    </div>
</div>

<div class="form-group row">
    <label class="col-form-label col-sm-2" for="cussaleytd">Sales YTD</label>
    <div class="col-sm-7">
        <input class="form-control" name="cussaleytd" value="<%= model.cussaleytd %>" />
    </div>
</div>

<div class="form-group row">
    <label class="col-form-label col-sm-2" for="cussaleprev">Previous Years Sales</label>
    <div class="col-sm-7">
        <input class="form-control" name="cussaleprev" value="<%= model.cussaleprev %>" />
    </div>
</div>